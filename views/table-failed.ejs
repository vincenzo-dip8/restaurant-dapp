            <div class="container px-4 px-lg-5">
                <div class="social d-flex justify-content-center">
                    <a class="m-3 mx-2" href="#!"><i class="fab fa-twitter"></i></a>
                    <a class="m-3 mx-2" href="#!"><i class="fab fa-facebook-f"></i></a>
                    <a class="m-3 mx-2" href="#!"><i class="fab fa-instagram"></i></a>
                </div>
            </div>
        </section>
